<?xml version="1.0" ?>
<launch>

    <arg name="is_sim" default="true" />

    <node pkg="ros_robotic_skin" name="activation_matrix" type="activation_matrix.py" output="screen" />
    <node pkg="ros_robotic_skin" name="imu_listener" type="imu_listener.py" output="screen" />
    <node pkg="ros_robotic_skin" name="joint_movement" type="joint_movement.py" args="$(arg is_sim)" output="screen" />

</launch>